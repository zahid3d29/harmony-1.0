<style>
    /* Create an active/current productFeaturedTablink class */
    .productFeaturedTab button.active {
        border-bottom: 2px solid #707070;
    }

    /* Style the productFeaturedTab content */
    .productFeaturedTabcontent {
        display: none;
        animation: fadeEffect 2s; 
    }

    @keyframes fadeEffect {
        from {opacity: 0;}
        to {opacity: 1;}
    }
    #product__overlay{
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        /* background-color: #E8E7E3; */
        overflow: hidden;
        width: 100%;
        height: 0;
        transition: .5s ease;
        z-index: 99;

    }
    #product__grid-wrapper:hover #product__overlay{
        height: 100%;
    }
</style>


{%- style -%}

  /* schema settings color  */
  {% for scheme in settings.color_schemes %}
    .harmony__color-{{ scheme.id }}{
      background: rgb({{ scheme.settings.background.red }},{{ scheme.settings.background.green }},{{ scheme.settings.background.blue }});
      color: rgb({{ scheme.settings.text.red }},{{ scheme.settings.text.green }},{{ scheme.settings.text.blue }});
    }
  {% endfor %}

  /* schema settings Badge Corner Radius  */
  .badge_corner_radius{
    border-radius: {{ settings.badge_corner_radius }}px;
  }
  .product__grid-card {
    padding: {{ settings.product_card_horizontal_offset }}px {{ settings.product_card_vertical_offset }}px;
    border-radius: {{ settings.product_card_corner_radius }}px;
    border-width: {{ settings.product_card_border_thickness }}px;
    border-color: {{ settings.product_card_border_color }}{{ settings.product_card_border_opacity }};
  }
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }
  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}


<section id="harmony--{{ section.id }}" class="section__product-featured harmony__color-{{ section.settings.color_scheme }}">
  <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8 section-{{ section.id }}-padding">
    <div class="flex items-center lg:items-baseline justify-between mb-8">
        <h2 class="product__featured-heading text-[27px] leading-[36px] lg:text-[50px] lg:leading-[56px]  font-semibold lg:max-w-3xl tracking-tight">{{ section.settings.heading }}</h2>
        <div class="product__featured-slide flex items-center">
            <button class="flex items-center py-[0.4375rem] pl-2 pr-2 lg:pr-3">
                <span class="lg:mr-2">
                    <a href="#" class="relative inline-flex items-center rounded-full text-[17px] leading-[35px] px-2 py-2 hover:harmony__color-{{ section.settings.color_scheme }} ring-1 ring-inset ring-gray-300 bg-transparent hover:harmony__color-{{ section.settings.color_scheme }} focus:z-20 focus:outline-offset-0">
                        <span class="sr-only">Previous</span>
                        <svg class="h-5 w-3" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                            <path fill-rule="evenodd" d="M12.79 5.23a.75.75 0 01-.02 1.06L8.832 10l3.938 3.71a.75.75 0 11-1.04 1.08l-4.5-4.25a.75.75 0 010-1.08l4.5-4.25a.75.75 0 011.06.02z" clip-rule="evenodd"></path>
                        </svg>
                    </a>
                </span>
                <span class="mx-2 lg:mx-2">
                    <div class="h-1.5 w-1.5 rounded-full bg-black"></div>
                </span>
                <span class="mx-2 lg:mx-2">
                    <div class="h-1.5 w-1.5 rounded-full bg-gray-300"></div>
                </span>
                <span class="mx-2 lg:mx-2">
                    <div class="h-1.5 w-1.5 rounded-full bg-gray-300"></div>
                </span>
                <span class="lg:ml-2 harmony__color-{{ section.settings.color_scheme }}">
                    <a href="#" class="relative inline-flex items-center rounded-full px-2 py-2 harmony__color-{{ section.settings.color_scheme }} hover:harmony__color-{{ section.settings.color_scheme }} bg-[#7070704d] hover:bg-gray-100 ring-1 ring-inset ring-[#7070704d] focus:z-20 focus:outline-offset-0">
                        <span class="sr-only">Next</span>
                        <svg class="h-5 w-3" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                        <path fill-rule="evenodd" d="M7.21 14.77a.75.75 0 01.02-1.06L11.168 10 7.23 6.29a.75.75 0 111.04-1.08l4.5 4.25a.75.75 0 010 1.08l-4.5 4.25a.75.75 0 01-1.06-.02z" clip-rule="evenodd"></path>
                        </svg>
                    </a>
                </span>
            </button>
        </div>
    </div>
    <div class="mx-auto">
      <div class="productFeaturedTab overflow:hidden bg-white-900 grid lg:border-t lg:border-b border-[#7070704d] mb-6 grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 xl:grid-cols-4">
          {% for block in section.blocks %}
              <button class="productFeaturedTablinks block text-[17px] leading-[35px]  h-[4rem] my-1 lg:my-0 bg-transparent outline-none lg:last:border-r-0 border-t border-b lg:border-r lg:border-b-0 border-[#7070704d] cursor-pointer transition duration-300 hover:border-b-2 hover:border-b-[#707070] {% if forloop.index == 1 %}active{% endif %}" onmouseover="openFtCats(event, 'pd_{{ block.settings.collection.handle }}')" {% if forloop.index == 1 %} id="defaultOpen"{% endif %}>{{ block.settings.collection.title }}</button>
          {% endfor %}
      </div>

      {% for block in section.blocks %}
        <div id="pd_{{ block.settings.collection.handle }}" class="productFeaturedTabcontent grid grid-cols-{{ section.settings.number_of_column }} gap-x-4 md:gap-x-6 gap-y-10 sm:grid-cols-{{ section.settings.number_of_column }} lg:grid-cols-{{ section.settings.products_per_row }}  xl:grid-cols-{{ section.settings.products_per_row }}  xl:gap-x-8" {% if forloop.index == 1 %}style="display:grid"{% endif %} >
          {%- for product in block.settings.collection.products limit:  section.settings.products_to_show  -%}

          {%- comment -%}  
          <div class="product__grid-wrapper {% if settings.product_card_style == 'card' %} product__grid-card {% endif %} group {% if settings.product_card_style == 'card' %} harmony__color-{{ settings.product_card_color_scheme }} {% endif %}" id="product__grid-wrapper">
            <div class="product__media relative aspect-h-1 aspect-w-1 w-full overflow-hidden bg-gray-200 xl:aspect-h-8 xl:aspect-w-7">
              {%- if section.settings.enable_stock_badge -%} 
              {%- if product.available -%}
                <span class="stock__status-sale harmony__color-{{ settings.sale_badge_color_scheme }} badge_corner_radius inline max-w-[90px] max-h-[25px] z-10 items-center ml-2 mt-2 px-2 pb-1 text-center text-[14px]">
                    Sale
                </span>
              {%- else -%}
                <span class="stock__status-sold-out harmony__color-{{ settings.sold_out_badge_color_scheme }} badge_corner_radius inline max-w-[90px] max-h-[25px] z-10 items-center ml-2 mt-2 px-2 pb-1 text-center text-[14px]">
                Sold out
                </span>
              {%- endif -%}
              {% endif %}
              <img src="{{ product.featured_media | image_url }}" alt="{{ product.title }}" class="h-full w-full object-cover object-center group-hover:opacity-75">
              <div id="product__overlay" class="harmony__color-{{ settings.product_card_overlay_color_scheme }}">
                <div class="flex h-full max-w-xl flex-col items-start justify-between">
                  {%- if section.settings.enable_quick_view -%}
                  <div class="flex items-center justify-center harmony__color-{{ settings.product_card_overlay_color_scheme }} gap-x-4 text-xs">
                    {%- if section.settings.offer_badge -%} <span class="sale-badge absolute z-50 left-3 top-3 inline-block items-start badge_corner_radius harmony__color-{{ settings.product_card_overlay_color_scheme }} py-1 px-2 font-semibold">40% off</span>{% endif %}
                    {%- if section.settings.show_wishlist -%}  
                    <button class="absolute right-2 top-3 text-sm font-semibold h-[35px] w-[35px] block harmony__color-{{ settings.product_card_overlay_color_scheme }} rounded-full px-1 pt-[5px]">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M21 8.25c0-2.485-2.099-4.5-4.688-4.5-1.935 0-3.597 1.126-4.312 2.733-.715-1.607-2.377-2.733-4.313-2.733C5.1 3.75 3 5.765 3 8.25c0 7.22 9 12 9 12s9-4.78 9-12Z"></path>
                        </svg>
                    <span class="sr-only  lg:ml-2 border border-transparent"></span>
                    </button>
                    {% endif %}
                  </div>
                  {% endif %}
                  {%- if section.settings.enable_quick_view -%}
                  <button type="submit" class="mx-auto flex w-[85%] items-center justify-center harmony__color-{{ settings.product_card_overlay_color_scheme }} px-4 py-4 mb-4 text-base font-medium">Quick View</button>
                  {% endif %}
                </div>
              </div>
            </div>
            <div class="product__info mt-4">
              <table class="table-auto w-full">
              <tbody>
                  <tr>
                  <td class="border-b border-[#707070] pr-2 py-2">
                      <a href="{{ product.url }}" class="product__title block">
                          <h3 class="product__title text-[16px] leading-[21px] font-medium">
                          {{ product.title }}
                          </h3>
                      </a>
                  </td>
                  <td class="border-b border-[#707070] text-right pl-2">
                      {%- if section.settings.show_price -%}  
                      <h4 class="product__price text-[13px] leading-[17px] text-right">{{ product.price | money }}</h4>
                      {% endif %}
                  </td>
                  </tr>
                  <tr>
                  <td class="pr-2 py-2">
                      {%- if section.settings.show_category -%}
                      {% for collection in product.collections limit: 1 %}
                          <div class="collection__name bg-transparent harmony__color-{{ section.settings.color_scheme }} text-[14px] leading-[19px] inline-block px-2 py-1.5 rounded-full collection-title">{{ collection.title }}</div>
                      {% endfor %}
                      {% endif %}
                  </td>
                  <td class="flex items-center justify-end text-right py-2 pl-2">
                      {%- if section.settings.show_reviews -%}
                      <div class="flex items-center justify-end text-[13px]">
                          <svg class="w-3 flex-shrink-0" x-state:on="Active" x-state:off="Default" x-state-description="Active: &quot;harmony__color-{{ section.settings.color_scheme }}&quot;, Default: &quot;text-gray-200&quot;" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                              <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd"></path>
                          </svg>
                          <svg class="w-3 flex-shrink-0" x-state-description="undefined: &quot;harmony__color-{{ section.settings.color_scheme }}&quot;, undefined: &quot;text-gray-200&quot;" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                              <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd"></path>
                          </svg>
                          <svg class="w-3 flex-shrink-0" x-state-description="undefined: &quot;harmony__color-{{ section.settings.color_scheme }}&quot;, undefined: &quot;text-gray-200&quot;" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                              <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd"></path>
                          </svg>
                          <svg class= w-3 flex-shrink-0 x-state-description="undefined: &quot;harmony__color-{{ section.settings.color_scheme }}&quot;, undefined: &quot;text-gray-200&quot;" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                              <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd"></path>
                          </svg>
                          <svg class="text-gray-200 w-3 flex-shrink-0" x-state-description="undefined: &quot;harmony__color-{{ section.settings.color_scheme }}&quot;, undefined: &quot;text-gray-200&quot;" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                              <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd"></path>
                          </svg>
                      </div>
                      <span class="sr-only">3.9 out of 5 stars</span>
                      <span class="product__feedback-count ml-3 text-[13px] leading-[17px] hover:text-gray-500">22</span>
                      {% endif %}
                  </td>
                  </tr>
              </tbody>
              </table>
            </div>
          </div>
          {%- endcomment -%}



          <div class="product__grid-wrapper {% if settings.product_card_style == 'card' %} product__grid-card {% endif %} group {% if settings.product_card_style == 'card' %} harmony__color-{{ section.settings.product_card_color_scheme }} {% else %} harmony__color-{{ section.settings.section_color_scheme }} {% endif %}" id="product__grid-wrapper">
            <div class="product__media relative aspect-h-1 aspect-w-1 w-full overflow-hidden xl:aspect-h-8 xl:aspect-w-7">
              {%- if section.settings.enable_stock_badge -%} 
                {%- if product.available -%}
                <span class="badge__sale harmony__color-{{ settings.sale_badge_color_scheme }} badge_corner_radius inline max-w-[90px] max-h-[25px] z-20 items-center ml-2 mt-2 px-2 pb-1 text-center text-[14px]">
                  Sale
                </span>
                {%- else -%}
                <span class="badge__sold-out harmony__color-{{ settings.sold_out_badge_color_scheme }} badge_corner_radius inline max-w-[90px] max-h-[25px] z-20 items-center ml-2 mt-2 px-2 pb-1 text-center text-[14px]">
                  Sold out
                </span>
                {%- endif -%}
              {% endif %}
              
              {% if product.featured_media %}
                {{ product.featured_media | image_url: width: 407, height: 513 |  image_tag }}
              {% else %}
                <img src="{{ 'cdn.shopify.com/s/files/1/0586/2499/9582/files/placeholder-images-image_large_7f671d3f-3a3a-4dc7-9545-5aa9fe3f02a5.webp?v=1716098263' | asset_url }}" width="407" height="513" alt="Fallback Image">
              {% endif %}
              
              <div id="product__overlay" class="harmony__color-{{ settings.product_card_overlay_color_scheme }}">
                <div class="flex h-full max-w-xl flex-col items-start justify-between">
                  {%- if section.settings.enable_quick_view -%}
                  <div class="flex items-center justify-center harmony__color-{{ settings.product_card_overlay_color_scheme }} gap-x-4 text-xs">
                    {%- if section.settings.offer_badge -%} <span class="badge__discount absolute z-14 left-3 top-3 inline-block items-start badge_corner_radius harmony__color-{{ settings.sale_offer_badge_color_scheme }} py-1 px-2 font-semibold">40% off</span>{% endif %}
                    {%- if section.settings.show_wishlist -%}  
                    <button class="absolute right-2 top-3 text-sm font-semibold h-[35px] w-[35px] block harmony__color-{{ settings.product_card_overlay_color_scheme }} rounded-full px-1">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M21 8.25c0-2.485-2.099-4.5-4.688-4.5-1.935 0-3.597 1.126-4.312 2.733-.715-1.607-2.377-2.733-4.313-2.733C5.1 3.75 3 5.765 3 8.25c0 7.22 9 12 9 12s9-4.78 9-12Z"></path>
                        </svg>
                    <span class="sr-only lg:ml-2 border border-transparent"></span>
                    </button>
                    {% endif %}
                  </div>
                  {% endif %}
                  {%- if section.settings.enable_quick_view -%}
                  <button type="submit" class="mx-auto flex w-[85%] items-center justify-center overlay__btn  px-4 py-4 mb-4 text-base font-medium">Quick View</button>
                  {% endif %}
                </div>
              </div>
            </div>
            <div class="product__info mt-4">
              <table class="table-auto w-full">
                <tbody>
                  <tr>
                    {% if product.title %}
                    <td class="border-b border-[#707070] pr-2 py-2">
                      <a href="{{ product.url }}" class="product__title block">
                        <h3 class="product__title text-[16px] leading-[21px] font-medium">
                        {{ product.title |  truncate: section.settings.number_of_letters_in_title }}
                        </h3>
                      </a>
                    </td>
                    {% endif %}
                    {%- if section.settings.show_price -%}  
                    <td class="border-b border-[#707070] text-right pl-2">
                      <h4 class="product__price text-[13px] leading-[17px] text-right">{{ product.price | money }}</h4>
                    </td>
                    {% endif %}
                  </tr>
                  <tr>
                    {% for collection in product.collections limit: 1 %}
                      {%- if section.settings.show_category and collection != blank -%}
                      <td class="pr-2 py-2">
                          <a href="{{ collection.url }}" class="collection__name bg-transparent {% if settings.product_card_style == 'card' %} harmony__color-{{ section.settings.product_card_color_scheme }} {% else %} harmony__color-{{ section.settings.section_color_scheme }} {% endif %} text-[14px] leading-[19px] inline-block px-2 py-1.5 border rounded-full collection-title">{{ collection.title |  truncate: 10}}</a>
                      </td>
                      {% endif %}
                    {% endfor %}
                    {%- if section.settings.show_reviews -%}
                    <td class="flex items-center {%- if section.settings.show_category != true or product.collections == blank -%} justify-start  {% else %} justify-end pl-2 {% endif %} text-right py-2 ">
                      <div class="flex items-center justify-end text-[13px] leading-[18px] bg-transparent {% if settings.product_card_style == 'card' %} harmony__color-{{ section.settings.product_card_color_scheme }} {% else %} harmony__color-{{ section.settings.section_color_scheme }} {% endif %}">
                        {%  render 'rating-star' %}
                      </div>
                      <span class="sr-only">3.9 out of 5 stars</span>
                      <span class="product__feedback-count ml-3 text-[13px] leading-[17px]">22</span>
                    </td>
                    {% endif %}
                  </tr>
                </tbody>
              </table>
            </div>
          </div>



          {% endfor %}
        </div>
      {% endfor %}
    </div>        
  </div>
</section>


{% schema %}
{
    "name": "Product Fetaured",
    "settings": [
        {
          "type": "color_scheme",
          "id": "color_scheme",
          "label": "Color Scheme",
          "default": "scheme-1"
        },
        {
          "type": "text",
          "id": "heading",
          "label": "Section Heading",
          "default": "Section Heading"
        },
        {
          "type": "checkbox",
          "id": "show_heeading",
          "label": "Show Heading",
          "default": true
        },
        {
          "type": "select",
          "id": "enable_carousel",
          "label": "Enable Carousel",
          "options": [
            {
              "value": "grid",
              "label": "Grid"
            },
            {
              "value": "carousel",
              "label": "Carousel"
            }
        ],
        "default": "grid"
      },
      {
        "type": "checkbox",
        "id": "show_price",
        "label": "Show Price",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "show_category",
        "label": "Show Category",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "show_reviews",
        "label": "Show Reviews",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "enable_stock_badge",
        "label": "Enable Stock Badge",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "offer_badge",
        "label": "Sale Badge",
        "default": true
      },
        {
        "type": "checkbox",
        "id": "show_wishlist",
        "label": "Show Wishlist",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "enable_quick_view",
        "label": "Enable Quick View",
        "default": true
      },
      {
        "type": "range",
        "id": "products_to_show",
        "label": "Products to show?",
        "min": 2,
        "max": 12,
        "step": 1,
        "default": 3
      },
      {
        "type": "range",
        "id": "products_per_row",
        "label": "Products per row",
        "min": 2,
        "max": 4,
        "step": 1,
        "default": 3
      },
      {
        "type": "select",
        "id": "mobile_layout",
        "label": "Mobile Layout",
        "options": [
          {
            "value": "grid",
            "label": "Grid"
          },
          {
            "value": "slider",
            "label": "Slider"
          }
        ],
        "default": "grid"
      },
      {
        "type": "select",
        "id": "number_of_column",
        "label": "Number of Column",
        "options": [
          {
            "value": "1",
            "label": "1"
          },
          {
            "value": "2",
            "label": "2"
          }
        ],
        "default": "1",
        "info": "Number of Products in one column on mobile"
      },
      {
          "type": "header",
          "content": "Section padding"
      },
      {
          "type": "range",
          "id": "padding_top",
          "min": 0,
          "max": 100,
          "step": 4,
          "unit": "px",
          "label": "Padding Top",
          "default": 36
      },
      {
          "type": "range",
